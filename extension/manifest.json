{
  "name": "Mocket",
  "version": "0.1",
  "manifest_version":2,
  "description": "Save songs to mocket.",
  "page_action": {
    "default_title": "Mocket"
  },
  "background": {
    "scripts": ["build/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["build/main.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs" ,
    "http://*/",
    "<all_urls>"
  ]
}
