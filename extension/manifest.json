{
  "name": "Mocket",
  "version": "0.1",
  "manifest_version":2,
  "description": "Save songs to mocket.",
  "page_action": {
    "default_title": "Mocket"
  },
  "background": {
    "scripts": ["build/background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["build/main.js"],
      "run_at": "document_end"
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "tabs" ,
    "http://mocket.in/",
    "http://localhot:3000/"
  ]
}
